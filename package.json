    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@etherpacks/dpack": "^0.0.31",
    "@nomiclabs/hardhat-ethers": "^2.1.0",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/parser": "^4.0.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7",
    "ethers": "^5.7.2",
    "hardhat": "^2.10.1",
    "minihat": "^0.0.6",
    "mocha": "^10.2.0",
    "prettier": "2.8.1",
    "prettier-plugin-solidity": "1.0.0-beta.19",
